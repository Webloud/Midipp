<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reonsaji.midipp">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fav"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.reonsaji.midipp.Search" />
        <activity android:name="com.reonsaji.midipp.searchview" />
        <activity android:name="com.reonsaji.midipp.Home" />
        <activity android:name="com.reonsaji.midipp.intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.reonsaji.midipp.fsave" />
        <activity android:name="com.reonsaji.midipp.main2"></activity>
    </application>

</manifest>